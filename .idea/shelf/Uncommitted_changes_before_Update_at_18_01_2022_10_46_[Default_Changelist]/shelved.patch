StompClient\r\n    private val TAG = NotificationVcallService::class.java.simpleName\r\n    private var wakeLock: PowerManager.WakeLock? = null\r\n    lateinit var jitsiIntent: Intent\r\n    lateinit var callIntent: Intent\r\n    private val prefs by inject<SharedPreferences>()\r\n    private val okHttpClient by inject<OkHttpClient>()\r\n\r\n    @SuppressLint(\"WakelockTimeout\")\r\n    private fun acquireLock() {\r\n        // we need this lock so our service gets not affected by Doze Mode\r\n        wakeLock =\r\n            (getSystemService(Context.POWER_SERVICE) as PowerManager).run {\r\n                newWakeLock(PowerManager.PARTIAL_WAKE_LOCK, \"NotificationVcallService::lock\").apply {\r\n                    acquire()\r\n                }\r\n            }\r\n    }\r\n\r\n    override fun onStartCommand(intent: Intent, flags: Int, startId: Int): Int {\r\n        var input = intent.getStringExtra(\"inputExtra\")\r\n        if(input == null){\r\n            val format = SimpleDateFormat(\"yyyy-MM-dd\")\r\n            val scheduledDate = prefs.getString(Constant.KEY_SCHEDULE_DATE,\"2022-01-18\")\r\n            val scheduledHour = prefs.getString(Constant.KEY_SCHEDULE_HOUR, \"14:30-15:30\")\r\n            val formatDate = SimpleDateFormat(\"dd-MMM-yyyy\")\r\n            val dateTime = try {\r\n                val date = format.parse(scheduledDate)\r\n                formatDate.format(date!!)\r\n            } catch (e: Exception) {\r\n                scheduledDate\r\n            }\r\n            input = \"Jadwal Videocall : \" + dateTime + \" \" + scheduledHour\r\n        }\r\n\r\n        val notificationIntent = Intent(this, MainActivity::class.java)\r\n        val pendingIntent = PendingIntent.getActivity(\r\n            this,\r\n            0, notificationIntent, 0\r\n        )\r\n        // val remoteViews = RemoteViews(applicationContext.getPackageName(), R.layout.custom_notif)\r\n        val notification = NotificationCompat.Builder(this, App.CHANNEL_ID)\r\n            .setContentTitle(\"Jadwal VideoCall\")\r\n            .setContentText(input)\r\n            .setSmallIcon(R.drawable.bion_logo_notification)\r\n            .setContentIntent(pendingIntent)\r\n            .build()\r\n        val mNotificationManager = getSystemService(NOTIFICATION_SERVICE) as NotificationManager\r\n\r\n        val sdf = SimpleDateFormat(\"yyyy-MM-dd\")\r\n        val scheduledDate = prefs.getString(Constant.KEY_SCHEDULE_DATE,\"\")\r\n        val startDate = sdf.parse(scheduledDate)\r\n\r\n        if(prefs.getString(Constant.KEY_USER_STATUS,\"\") == Constant.KYC_WAITING &&\r\n            sdf.parse(sdf.format(Date())).time == startDate.time) {\r\n            if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.O) {\r\n                val channel = NotificationChannel(\r\n                    App.CHANNEL_ID,\r\n                    App.CHANNEL_NAME,\r\n                    NotificationManager.IMPORTANCE_DEFAULT\r\n                )\r\n                mNotificationManager.createNotificationChannel(channel)\r\n                NotificationCompat.Builder(this, App.CHANNEL_ID)\r\n            }\r\n            startForeground(1, notification)\r\n            stomp()\r\n            acquireLock()\r\n        }\r\n\r\n//        Log.d(\"NotificationVcallService\",\"onStartComand\")\r\n//        Log.d(\"NotificationVcallService\",\"\"+prefs.getBoolean(Constant.USER_IN_KYC_WAITING,false))\r\n        /*\r\n        Handler(mainLooper).postDelayed(Runnable {\r\n            if(prefs.getBoolean(Constant.USER_IN_KYC_WAITING,false) != null) {\r\n                if (!prefs.getBoolean(Constant.USER_IN_KYC_WAITING, false)) {\r\n                    stopForeground(true)\r\n                }\r\n            }\r\n        },3000)\r\n         */\r\n\r\n        return if(prefs.getString(Constant.KEY_USER_STATUS,\"\") == Constant.KYC_WAITING && sdf.parse(sdf.format(Date())).time == startDate.time) {\r\n            START_STICKY\r\n        } else {\r\n            START_NOT_STICKY\r\n        }\r\n    }\r\n\r\n    override fun onBind(intent: Intent): IBinder? {\r\n        return null\r\n    }\r\n\r\n    override fun onTaskRemoved(rootIntent: Intent?) {\r\n        if(prefs.getString(Constant.KEY_USER_STATUS,\"\") == Constant.KYC_WAITING) {\r\n            val restartServiceIntent = Intent(applicationContext, this.javaClass)\r\n            restartServiceIntent.setPackage(packageName)\r\n            startService(restartServiceIntent)\r\n        }else{\r\n            val stopServiceIntent = Intent(applicationContext, this.javaClass)\r\n            stopServiceIntent.setPackage(packageName)\r\n            stopService(stopServiceIntent)\r\n        }\r\n        super.onTaskRemoved(rootIntent)\r\n    }\r\n\r\n    private fun getUnsafeOkHttpClient(): OkHttpClient.Builder? {\r\n        return try {\r\n            // Create a trust manager that does not validate certificate chains\r\n            val trustAllCerts: Array<TrustManager> = arrayOf<TrustManager>(\r\n                object : X509TrustManager {\r\n                    @Throws(CertificateException::class)\r\n                    override fun checkClientTrusted(\r\n                        chain: Array<X509Certificate?>?,\r\n                        authType: String?\r\n                    ) {\r\n                    }\r\n\r\n                    @Throws(CertificateException::class)\r\n                    override fun checkServerTrusted(\r\n                        chain: Array<X509Certificate?>?,\r\n                        authType: String?\r\n                    ) {\r\n                    }\r\n\r\n                    override fun getAcceptedIssuers(): Array<X509Certificate> {\r\n                        return arrayOf()\r\n                    }\r\n                }\r\n            )\r\n\r\n            // Install the all-trusting trust manager\r\n            val sslContext: SSLContext = SSLContext.getInstance(\"SSL\")\r\n            sslContext.init(null, trustAllCerts, SecureRandom())\r\n\r\n            // Create an ssl socket factory with our all-trusting manager\r\n            val sslSocketFactory: SSLSocketFactory = sslContext.getSocketFactory()\r\n            val builder: OkHttpClient.Builder = OkHttpClient.Builder()\r\n            builder.sslSocketFactory(sslSocketFactory, trustAllCerts[0] as X509TrustManager)\r\n            builder.hostnameVerifier(HostnameVerifier { hostname, session -> true })\r\n            builder\r\n        } catch (e: Exception) {\r\n            throw RuntimeException(e)\r\n        }\r\n    }\r\n\r\n    fun stomp(){\r\n        Handler(Looper.myLooper()!!).postDelayed(Runnable {\r\n            val sdf = SimpleDateFormat(\"yyyy-MM-dd\")\r\n            val scheduledDate = prefs.getString(Constant.KEY_SCHEDULE_DATE,\"\")\r\n            val startDate = sdf.parse(scheduledDate)\r\n            if(prefs.getString(Constant.KEY_USER_STATUS,\"\") == Constant.KYC_WAITING && sdf.parse(sdf.format(Date())).time == startDate.time){\r\n                val stopServiceIntent = Intent(applicationContext, this.javaClass)\r\n                stopServiceIntent.setPackage(packageName)\r\n                stopService(stopServiceIntent)\r\n            }\r\n        },3000)\r\n\r\n        mStompClient = Stomp.over(\r\n            Stomp.ConnectionProvider.OKHTTP,\r\n            BASE_VCS_WEBSOCKET + \"websocketnotification\"\r\n        )\r\n        mStompClient.withClientHeartbeat(10000).withServerHeartbeat(10000)\r\n        resetSubscriptions()\r\n        val dispLifecycle = mStompClient.lifecycle()\r\n            .subscribeOn(Schedulers.io())\r\n            .observeOn(AndroidSchedulers.mainThread())\r\n            .subscribe { lifecycleEvent: LifecycleEvent ->\r\n                when (lifecycleEvent.type) {\r\n                    LifecycleEvent.Type.OPENED -> {\r\n                        //toast(\"Stomp connection opened\")\r\n                        prepareOpenVcall()\r\n                        //prepareCloseVcall()\r\n                    }\r\n                    LifecycleEvent.Type.ERROR -> {\r\n                        Log.e(TAG, \"Stomp connection error\", lifecycleEvent.exception)\r\n//                        toast(\"Webcosocket connection error\")\r\n                        //try to reconnect\r\n                        mStompClient.reconnect()\r\n                    }\r\n                    LifecycleEvent.Type.CLOSED -> {\r\n//                        toast(\"Webcosocket connection closed\")\r\n                        resetSubscriptions()\r\n                    }\r\n                    LifecycleEvent.Type.FAILED_SERVER_HEARTBEAT -> {\r\n//                        toast(\"Webcosocket failed server heartbeat\")\r\n                        mStompClient.reconnect()\r\n                    }\r\n                }\r\n            }\r\n        compositeDisposable!!.add(dispLifecycle)\r\n        mStompClient.connect()\r\n    }\r\n\r\n    fun prepareOpenVcall(){\r\n        compositeDisposable!!.add(mStompClient.topic(\r\n            \"/open/changes/\" + prefs.getString(\r\n                Constant.KEY_DEVICE_ID,\r\n                \"\"\r\n            )\r\n        )\r\n            .subscribeOn(Schedulers.io())\r\n            .observeOn(AndroidSchedulers.mainThread())\r\n            .subscribe(\r\n                { message ->\r\n                    if (message != null) {\r\n                        val messageDecrpt = LoginAuthService.decrpytMsg(message.payload)\r\n                        val notif = moshi.adapter(NotifVcallDto::class.java).fromJson(\r\n                            messageDecrpt\r\n                        )\r\n                        showNotification(notif!!.roomId!!)\r\n                    }\r\n                }, { err -> Log.d(\"close\", err.message!!) }\r\n            ))\r\n    }\r\n\r\n    fun prepareCloseVcall(){\r\n        compositeDisposable!!.add(mStompClient.topic(\r\n            \"/close/changes/\" + prefs.getString(\r\n                Constant.KEY_DEVICE_ID,\r\n                \"\"\r\n            )\r\n        )\r\n            .subscribeOn(Schedulers.io())\r\n            .observeOn(AndroidSchedulers.mainThread())\r\n            .subscribe(\r\n                { message ->\r\n                    if (message != null) {\r\n//                        if (jitsiIntent != null) {\r\n                        if (callIntent != null) {\r\n                            //PendingIntent.getActivity(baseContext, 101010, jitsiIntent, PendingIntent.FLAG_UPDATE_CURRENT).cancel()\r\n                            val mainActivity = Intent()\r\n                            mainActivity.setClass(applicationContext, MainActivity::class.java)\r\n                            mainActivity.addFlags(Intent.FLAG_ACTIVITY_NEW_TASK or Intent.FLAG_ACTIVITY_CLEAR_TASK)\r\n//                            Handler(Looper.myLooper()!!).postDelayed(Runnable {\r\n                                startActivity(mainActivity)\r\n//                                System.exit(0)//force close all\r\n//                            }, 1000)\r\n                            val notificationManager = App.appContext!!.getSystemService(Context.NOTIFICATION_SERVICE) as NotificationManager\r\n                            notificationManager.cancel(Constant.NOTIFICATION_ID)\r\n                        }\r\n                    }\r\n                }, { err -> Log.d(\"close\", err.message!!) }\r\n            ))\r\n    }\r\n\r\n    private fun resetSubscriptions() {\r\n        if (compositeDisposable != null) {\r\n            compositeDisposable!!.dispose()\r\n        }\r\n        compositeDisposable = CompositeDisposable()\r\n    }\r\n\r\n    override fun onDestroy() {\r\n        mStompClient.disconnect()\r\n        mRestPingDisposable?.dispose()\r\n        compositeDisposable?.dispose()\r\n        super.onDestroy()\r\n    }\r\n\r\n    private fun toast(text: String) {\r\n        Log.i(TAG, text)\r\n        Toast.makeText(this, text, Toast.LENGTH_LONG).show()\r\n    }\r\n\r\n\r\n    //langsung vcall\r\n    private fun jitsiScreen(room: String): PendingIntent? {\r\n        callIntent = Intent(applicationContext, IncomingCallActivity::class.java)\r\n        callIntent.putExtra(\"jitsi_room\", room)\r\n        prepareCloseVcall()\r\n//        return PendingIntent.getActivity(baseContext, 101010, jitsiIntent, PendingIntent.FLAG_UPDATE_CURRENT)\r\n        return PendingIntent.getActivity(\r\n            baseContext,\r\n            101010,\r\n            callIntent,\r\n            PendingIntent.FLAG_UPDATE_CURRENT\r\n        )\r\n    }\r\n\r\n    private fun splashToJitsi(room: String, force: String? = \"no\"): PendingIntent? {\r\n        callIntent = Intent(applicationContext, SplashActivity::class.java)\r\n        callIntent.putExtra(\"jitsi_room\", room)\r\n        callIntent.putExtra(\"jitsi_force\", force)\r\n        prepareCloseVcall()\r\n//        return PendingIntent.getActivity(baseContext, 101010, jitsiIntent, PendingIntent.FLAG_UPDATE_CURRENT)\r\n        return PendingIntent.getActivity(\r\n            baseContext,\r\n            101010,\r\n            callIntent,\r\n            PendingIntent.FLAG_UPDATE_CURRENT\r\n        )\r\n    }\r\n\r\n    private fun showNotification(room: String) {\r\n        val CHANNEL_ID = \"1234\"\r\n\r\n        val soundUri =\r\n            RingtoneManager.getDefaultUri(RingtoneManager.TYPE_RINGTONE)\r\n        val mNotificationManager =\r\n            applicationContext.getSystemService(NOTIFICATION_SERVICE) as NotificationManager\r\n        val audioAttributes = AudioAttributes.Builder()\r\n            .setContentType(AudioAttributes.CONTENT_TYPE_SONIFICATION)\r\n            .setUsage(AudioAttributes.USAGE_NOTIFICATION)\r\n            .build()\r\n        //For API 26+ you need to put some additional code like below:\r\n        val mChannel: NotificationChannel\r\n        if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.O) {\r\n            mChannel = NotificationChannel(\r\n                CHANNEL_ID,\r\n                \"MESTIKA_CHANNEL\",\r\n                NotificationManager.IMPORTANCE_HIGH\r\n            )\r\n//            mChannel.lightColor = Color.GRAY\r\n            mChannel.enableLights(true)\r\n            mChannel.description = \"MESTIKA_CHANNEL_DESCRIPTION\"\r\n            mChannel.setSound(soundUri, audioAttributes)\r\n            mNotificationManager?.createNotificationChannel(mChannel)\r\n        }\r\n\r\n        //General code:\r\n        val status = NotificationCompat.Builder(\r\n            applicationContext, CHANNEL_ID\r\n        )\r\n        status.setWhen(System.currentTimeMillis())\r\n            .setSmallIcon(R.drawable.bion_logo_notification) //.setOnlyAlertOnce(true)\r\n            .setContentTitle(getString(R.string.app_name))\r\n            .setContentText(resources.getString(R.string.txt_teks_vcall_notification))\r\n            .setVibrate(longArrayOf(100, 200, 300, 400, 500, 400, 300, 200, 400))\r\n            .setOnlyAlertOnce(false)\r\n            .setDefaults(Notification.DEFAULT_LIGHTS and Notification.FLAG_INSISTENT )\r\n            .setSound(RingtoneManager.getDefaultUri(RingtoneManager.TYPE_RINGTONE))\r\n            .setContentIntent(jitsiScreen(room))\r\n            .setPriority(NotificationCompat.PRIORITY_MAX)\r\n            .setCategory(NotificationCompat.CATEGORY_CALL)\r\n            .setFullScreenIntent(jitsiScreen(room), true)\r\n            .setOngoing(true)\r\n            .addAction(0,\"Accept\",splashToJitsi(room, \"force\"))\r\n            .addAction(0,\"Decline\",PendingIntent.getActivity(\r\n                applicationContext,\r\n                101010,\r\n                Intent(applicationContext, MainActivity::class.java),\r\n                PendingIntent.FLAG_UPDATE_CURRENT\r\n            ))\r\n\r\n        mNotificationManager.notify(Constant.NOTIFICATION_ID, status.build())\r\n    }\r\n\r\n}
